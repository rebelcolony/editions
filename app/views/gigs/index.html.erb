
<div id="gigs_table">
<span class="admin_links"><%= link_to 'Create new gig', new_gig_path %></span>
<table>
<% @gigs.each do |gig| %>
  <tr>	
		<td><%= gig.date.strftime("%e %B %Y") %></td>
    <td><%= link_to(gig.title, gig) %></td>
    <td><%= gig.venue %></td>
    <td><%= gig.city %></td>
    <td><%= gig.country %></td>
    <td><span class="admin_links"><%= link_to 'Edit', edit_gig_path(gig) %></span></td>
    <td><span class="admin_links"><%= link_to 'Destroy', gig, :confirm => 'Are you sure?', :method => :delete %></span></td>
  </tr>
<% end %>
</table>
</div><!-- END #gigs_table -->