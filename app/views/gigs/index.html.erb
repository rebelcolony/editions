<h1>Listing gigs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Venue</th>
    <th>City</th>
    <th>Country</th>
    <th>Date</th>
    <th>Description</th>
    <th>Map</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @gigs.each do |gig| %>
  <tr>
    <td><%= gig.title %></td>
    <td><%= gig.venue %></td>
    <td><%= gig.city %></td>
    <td><%= gig.country %></td>
    <td><%= gig.date %></td>
    <td><%= gig.description %></td>
    <td><%= gig.map %></td>
    <td><%= link_to 'Show', gig %></td>
    <td><%= link_to 'Edit', edit_gig_path(gig) %></td>
    <td><%= link_to 'Destroy', gig, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Create new gig', new_gig_path %>
